{
  "schemaVersion": 1,
  "id": "khasm",
  "version": "${version}",
  "name": "Khasm",
  "description": "a concern library that does what Mixin can't",
  "authors": [
    "P03W",
    "sschr15"
  ],
  "contact": {
    "sources": "https://github.com/P03W/khasm",
    "issues": "https://github.com/P03W/khasm/issues"
  },
  "license": "MPL-2.0",
  "icon": "assets/khasm/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      {
        "adapter": "kotlin",
        "value": "net.khasm.init.KhasmInit"
      }
    ]
  },
  "depends": {
    "fabricloader": ">=0.10.8",
    "minecraft": "*",
    "fabric-language-kotlin": ">=1.5.0"
  },
  "custom": {
    "modmenu:api": true
  },
  "languageAdapters": {
    "fakekhasmlaunchlangadapter": "net.khasm.init.KhasmRelauncher"
  }
}
